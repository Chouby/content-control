{
	"extends": "./tsconfig.base.json",
	"compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			"@components/*": [
				"components/*",
			],
			"@components": [
				"components",
			],
			"@data": [
				"data/",
			],
			"@data/*": [
				"data/*",
			],
			"@icons": [
				"icons"
			],
			"@utils": [
				"utils"
			],
		},
	},
	"include": [
		"src/**/*"
	]
}