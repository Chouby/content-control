<psalm
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://getpsalm.org/schema/config"
    xsi:schemaLocation="https://getpsalm.org/schema/config vendor/vimeo/psalm/config.xsd"
    xmlns:xi="http://www.w3.org/2001/XInclude"

    errorLevel="8"
    runTaintAnalysis="false"
    disableVarParsing="false"
    findUnusedVariablesAndParams="false"
    findUnusedCode="false"
    findUnusedBaselineEntry="false"
    allowBackwardsIncompatibleChanges="false"
    resolveFromConfigFile="true"
    phpVersion="5.6"
>
    <projectFiles>
        <file name="content-control.php" />
        <directory name="inc" />
        <directory name="classes" />
    </projectFiles>
    <extraFiles>
        <directory name="vendor-prefixed" />
    </extraFiles>
    <stubs>
        <file name="bin/stubs/elementor.php" />
        <file name="bin/stubs/query-monitor.php" />
        <file name="vendor/php-stubs/wordpress-stubs/wordpress-stubs.php" />
    </stubs>
</psalm>